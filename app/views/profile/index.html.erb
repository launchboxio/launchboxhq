<h1>Profile</h1>

<% @access_tokens.each do |access_token| %>
  <%= access_token.created_at %>
  <%= access_token.scopes %>
  <%= access_token.expires_at %>
<% end %>

<button type="button" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#exampleModal">
  Launch demo modal
</button>
<%= render "access_token" %>
